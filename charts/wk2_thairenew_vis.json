{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "title": {
    "text": "Renewables Output in Thailand",
    "anchor": "start",
    "subtitle": "Output, GWh, | IEA via ECO-API",
    "fontSize": "14",
    "subtitleFontSize": "12",
    "color": "#000000",
    "subtitleColor": "#000000"
  },
  "height": 250,
  "width": "container",
  "autosize": {"type": "fit", "contains": "padding", "resize": false},
  "background": "#ffffff",
  "view": {"fill": "#ffffff", "strokeOpacity": 0},
  "data": {
    "url": "https://api.economicsobservatory.com/tha/rGWh?vega",
    "format": {"type": "json"}
  },
  "mark": {"type": "line", "color": "#2f5fd0", "strokeWidth": "2"},
  "encoding": {
    "x": {
      "field": "date",
      "type": "temporal",
      "axis": {"title": "", "titleColor": "#122B39", "grid": false}
    },
    "y": {
      "field": "value",
      "scale": {"domain": [0, 70000]},
      "type": "quantitative",
      "axis": {"title": "", "titleColor": "#122B39", "grid": false}
    }
  },
  "config": {}
}