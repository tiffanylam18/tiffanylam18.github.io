{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "title": {
    "text": "Unemployment in Vietnam",
    "anchor": "start",
    "subtitle": "Rate, % | Source: GSO (WB) through ECO API",
    "fontSize": "14",
    "subtitleFontSize": "12",
    "color": "#000000",
    "subtitleColor": "#000000"
  },
  "height": "250",
  "width": "container",
  "autosize": {"type": "fit", "contains": "padding", "resize": false},
  "background": "#ffffff",
  "view": {"fill": "#ffffff", "strokeOpacity": 0},
  "data": {
    "url": "https://api.economicsobservatory.com/vnm/unem?vega",
    "format": {"type": "json"}
  },
  "mark": {"type": "line", "color": "#ec5b36", "strokeWidth": "2"},
  "encoding": {
    "x": {
      "field": "date",
      "type": "temporal",
      "axis": {"title": "", "titleColor": "#122B39", "grid": false}
    },
    "y": {
      "field": "value",
      "type": "quantitative",
      "scale": {"domain": [0, 3.5]},
      "axis": {"title": "", "titleColor": "#122B39", "grid": false}
    }
  },
  "config": {}
}