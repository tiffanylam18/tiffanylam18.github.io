{
  "$schema": "https://vega.github.io/schema/vega-lite/v6.json",
  "width": 500,
  "title": {
    "text": "Vietnam Leads Southeast Asia in Renewable Energy Production (2023)",
    "subtitle": "With 119 TWh, Vietnam produces more than Thailand, Malaysia, and Philippines combined",
    "anchor": "start"
  },
  "data": {
    "url": "https://raw.githubusercontent.com/tiffanylam18/tiffanylam18.github.io/main/data/wk5_seasia_renew.csv"
  },
  "mark": {"type": "bar", "opacity": 0.9},
  "encoding": {
    "x": {
      "field": "Location",
      "type": "nominal",
      "sort": ["Vietnam", "Malaysia", "Indonesia", "Thailand", "Philippines"],
      "axis": {"title": null, "domain": false, "labelAngle": 0}
    },
    "y": {
      "field": "TWh",
      "type": "quantitative",
      "stack": "zero",
      "axis": {"title": "Renewable Energy (TWh)", "domain": false}
    },
    "color": {
      "field": "Source",
      "type": "nominal",
      "scale": {
        "domain": ["Hydro", "Solar", "Wind", "Geothermal", "Biomass"],
        "range": ["#2E86AB", "#F77F00", "#06A77D", "#C1121F", "#6A994E"]
      },
      "legend": {"title": "Source", "orient": "right"}
    }
  },
  "config": {}
}